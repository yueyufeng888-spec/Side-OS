{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.1.0",
  "default_locale": "zh_CN",
  "description": "__MSG_extensionDescription__",
  "side_panel": {
    "default_path": "sidepanel.html"
  },
 "permissions": [
    "sidePanel",
    "declarativeNetRequest", 
    "declarativeNetRequestWithHostAccess",
    "storage",
    "contextMenus" 
  ],
"oauth2": {
    "client_id": "您的_CLIENT_ID_在这里.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.appdata"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],"web_accessible_resources": [
    {
      "resources": ["icons/*.png", "icons/*.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "打开/关闭 Side OS"
    }
  },
  "action": {
    "default_title": "打开 Side OS"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}